        <activity android:name=".adapter.AdapterActivity">
            <intent-filter>
                <action android:name="com.action.adapter"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".model.ModelActivity">
            <intent-filter>
                <action android:name="com.action.model"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".iterator.IteratorActivity">
            <intent-filter>
                <action android:name="com.action.iterator"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>