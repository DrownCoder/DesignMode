        </activity>
        <activity android:name=".factory.FactoryActivity">
            <intent-filter>
                <action android:name="com.action.factory"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".singleton.SingletonActivity">
            <intent-filter>
                <action android:name="com.action.singleton"/>

